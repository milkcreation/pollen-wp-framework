{
  "name": "pollen-solutions/wp-framework",
  "description": "Pollen Wordpress Framework Component.",
  "homepage": "https://www.presstify.com/pollen-solutions/wp-framework/",
  "keywords": [
    "presstify",
    "plugins",
    "wordpress",
    "framework"
  ],
  "time": "2021-06-15",
  "license": "MIT",
  "authors": [
    {
      "name": "Jordy Manner",
      "email": "jordy@presstify.com",
      "role": "Developper"
    }
  ],
  "repositories": [
    {
      "type": "composer",
      "url": "https://composer.presstify.com/pollen-solutions/"
    }
  ],
  "require": {
    "composer/installers": "~1.0",
    "pollen-solutions/framework": "^1.0",
    "pollen-solutions/wp-comment": "^1.0",
    "pollen-solutions/wp-db": "^1.0",
    "pollen-solutions/wp-hook": "^1.0",
    "pollen-solutions/wp-kernel": "^1.0",
    "pollen-solutions/wp-post": "^1.0",
    "pollen-solutions/wp-term": "^1.0",
    "pollen-solutions/wp-user": "^1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.0",
    "roave/security-advisories": "dev-latest"
  },
  "autoload": {
    "psr-4": {
      "Pollen\\WpFramework\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Pollen\\WpFramework\\Test\\": "tests"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  }
}
